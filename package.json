{
  "name": "nuxt-pdfmake",
  "version": "0.0.3",
  "description": "Easily generate PDFs in Nuxt.js using pdfmake",
  "repository": "your-org/my-module",
  "author": {
    "name": "Behon Baker",
    "email": "behon.baker@yahoo.com",
    "url": "https://behonbaker.com/"
  },
  "homepage": "https://nuxt-pdfmake.behonbaker.com",
  "keywords": [
    "pdfmake",
    "nuxt",
    "nuxt-module",
    "nuxt-pdfmake"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "start:dev": "node playground/.output/server/index.mjs",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.3",
    "pdfmake": "^0.2.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.6.1",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.3",
    "@nuxt/test-utils": "^3.5.3",
    "@nuxthq/ui": "^2.4.0",
    "@types/node": "^20.3.1",
    "@types/pdfmake": "^0.2.2",
    "changelogen": "^0.5.3",
    "eslint": "^8.43.0",
    "nuxt": "^3.5.3",
    "vitest": "^0.31.4"
  }
}