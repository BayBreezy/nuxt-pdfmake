{
  "name": "nuxt-pdfmake",
  "version": "0.0.16",
  "description": "Easily generate PDFs in Nuxt.js using pdfmake",
  "repository": "https://github.com/BayBreezy/nuxt-pdfmake",
  "author": {
    "name": "Behon Baker",
    "email": "behon.baker@yahoo.com",
    "url": "https://behonbaker.com/"
  },
  "homepage": "https://nuxt-pdfmake.behonbaker.com",
  "keywords": [
    "pdfmake",
    "nuxt",
    "nuxt-module",
    "nuxt-pdfmake"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "start:dev": "yarn prepack && yarn dev:build && node playground/.output/server/index.mjs",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "pdfmake": "^0.2.9"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.3",
    "@nuxt/test-utils": "^3.11.0",
    "@types/node": "^20.11.20",
    "@types/pdfmake": "^0.2.9",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.10.3",
    "postcss-custom-properties": "^13.3.5",
    "vitest": "^1.3.1"
  }
}